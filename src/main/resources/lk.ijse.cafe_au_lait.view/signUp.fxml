<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="ancPane" prefHeight="379.0" prefWidth="718.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.cafe_au_lait.controller.SignUpController">
   <children>
      <ImageView disable="true" fitHeight="415.0" fitWidth="743.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../lk.ijse.cafe_au_lait.assets/sign.png" />
         </image>
      </ImageView>
      <AnchorPane layoutX="44.0" layoutY="24.0" prefHeight="375.0" prefWidth="285.0" styleClass="loginForm" stylesheets="@../lk.ijse.cafe_au_lait.style/style.css">
         <children>
            <TextField fx:id="usernameTxt" focusTraversable="false" layoutX="59.0" layoutY="146.0" prefHeight="25.0" prefWidth="202.0" promptText="Username" styleClass="userNameField" stylesheets="@../lk.ijse.cafe_au_lait.style/style.css">
               <font>
                  <Font name="Times New Roman Bold" size="14.0" />
               </font>
            </TextField>
            <PasswordField fx:id="passwordTxt" focusTraversable="false" layoutX="59.0" layoutY="203.0" prefHeight="25.0" prefWidth="202.0" promptText="Password" styleClass="userNameField" stylesheets="@../lk.ijse.cafe_au_lait.style/style.css">
               <font>
                  <Font name="Times New Roman Bold" size="14.0" />
               </font>
            </PasswordField>
            <Text fill="#dfa47e" layoutX="81.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="loginName" text="Sign Up">
               <font>
                  <Font name="Times New Roman Bold" size="36.0" />
               </font>
            </Text>
            <ImageView fitHeight="32.0" fitWidth="34.0" layoutX="14.0" layoutY="147.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../lk.ijse.cafe_au_lait.assets/icons8-male-user.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="32.0" fitWidth="32.0" layoutX="14.0" layoutY="203.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../lk.ijse.cafe_au_lait.assets/icons8-lock.png" />
               </image>
            </ImageView>
            <JFXButton fx:id="loginBtn" buttonType="RAISED" layoutX="178.0" layoutY="304.0" onAction="#loginClick" prefHeight="25.0" prefWidth="83.0" style="-fx-background-color: #dfa47e; -fx-background-radius: 10;" text="Sign up" textFill="#7b3927">
               <font>
                  <Font name="Times New Roman Bold" size="14.0" />
               </font>
            </JFXButton>
            <Hyperlink fx:id="alreadyHaveAnAccount" layoutX="63.0" layoutY="337.0" onAction="#alreadyHaveAnAccountClick" text="Already have an account" textFill="#dfa47e">
               <font>
                  <Font name="Times New Roman Bold" size="14.0" />
               </font>
            </Hyperlink>
            <TextField fx:id="emailTxt" focusTraversable="false" layoutX="59.0" layoutY="90.0" prefHeight="25.0" prefWidth="202.0" promptText="E-mail" styleClass="userNameField" stylesheets="@../lk.ijse.cafe_au_lait.style/style.css">
               <font>
                  <Font name="Times New Roman Bold" size="14.0" />
               </font>
            </TextField>
            <ImageView fitHeight="32.0" fitWidth="34.0" layoutX="14.0" layoutY="90.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../lk.ijse.cafe_au_lait.assets/icons8-mail-48.png" />
               </image>
            </ImageView>
            <PasswordField fx:id="passwordTxt1" focusTraversable="false" layoutX="59.0" layoutY="257.0" prefHeight="25.0" prefWidth="202.0" promptText="Confirm Password" styleClass="userNameField" stylesheets="@../lk.ijse.cafe_au_lait.style/style.css">
               <font>
                  <Font name="Times New Roman Bold" size="14.0" />
               </font>
            </PasswordField>
            <ImageView fitHeight="32.0" fitWidth="32.0" layoutX="14.0" layoutY="257.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../lk.ijse.cafe_au_lait.assets/icons8-confirm-48.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
